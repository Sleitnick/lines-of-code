{
	"name": "roblox-ts-plugin",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"**/process-actor",
		"**/*.spec.lua"
	],
	"tree": {
		"$className": "Folder",
		"plugin": {
			"$path": "out/plugin.server.luau"
		},
		"config": {
			"$path": "out/config.luau"
		},
		"include": {
			"$path": "include",
			"node_modules": {
				"$className": "Folder",
				"@rbxts": {
					"$path": "node_modules/@rbxts"
				},
				"@rbxutil": {
					"$path": "node_modules/@rbxutil"
				}
			}
		},
		"frontend": {
			"$path": "out/frontend"
		},
		"backend": {
			"$path": "out/backend",
